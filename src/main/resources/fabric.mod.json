{
  "schemaVersion": 1,
  "id": "dash",
  "version": "${version}",
  "name": "Dash",
  "description": "Destroying loading times one cache at a time.",
  "authors": [
    "alphasucks"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/dash/icon.png",
  "environment": "client",
  "accessWidener" : "dash.accesswidener",
  "entrypoints": {
    "client": [
      "net.quantumfusion.dash.client.DashClient"
    ],
    "main": [
      "net.quantumfusion.dash.Dash"
    ]
  },
  "mixins": [
    "dash.mixins.json"
  ],
  "custom": {
    "sodium:options": {
      "mixin.features.model": false
    }
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": ">=1.16"
  }
}
